{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "node": ">=16.0.0 <20.0.0",
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr --disable-host-check",
    "predeploy": "npm run build:prod",
    "deploy": "ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "husky install",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.8",
    "@angular/cdk": "16.2.7",
    "@angular/common": "^16.2.8",
    "@angular/compiler": "^16.2.8",
    "@angular/core": "^16.2.8",
    "@angular/forms": "^16.2.8",
    "@angular/material": "^16.2.7",
    "@angular/material-moment-adapter": "^16.2.7",
    "@angular/platform-browser": "^16.2.8",
    "@angular/platform-browser-dynamic": "^16.2.8",
    "@angular/router": "^16.2.8",
    "@fortawesome/fontawesome-free": "6.3.0",
    "@ng-matero/extensions": "^16.1.1",
    "@ng-matero/extensions-moment-adapter": "^16.0.0",
    "@ngx-formly/core": "^6.2.1",
    "@ngx-formly/material": "^6.2.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/lodash": "^4.14.200",
    "angular-in-memory-web-api": "~0.16.0",
    "apexcharts": "^3.41.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.11.1",
    "crypto-browserify": "^3.12.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "ngx-permissions": "^15.0.1",
    "ngx-progressbar": "^11.1.0",
    "ngx-toastr": "^17.0.2",
    "photoviewer": "^3.8.1",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.1",
    "screenfull": "^6.0.2",
    "sweetalert2": "^11.6.13",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.8",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^16.2.5",
    "@angular/compiler-cli": "^16.2.8",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcryptjs": "^2.4.5",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "angular-cli-ghpages": "^1.0.7",
    "cpr": "^3.0.1",
    "eslint": "^8.51.0",
    "gulp": "^4.0.2",
    "gulp-each": "^0.5.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^14.0.1",
    "parse5": "^7.1.2",
    "prettier": "^3.0.3",
    "protractor": "^7.0.0",
    "rimraf": "5.0.5",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.3",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
